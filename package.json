{
  "name": "actions-template",
  "version": "1.0.0",
  "description": "Learn how to use GitHub Actions!",
  "main": "index.js",
  "directories": {
    "test": "__test__",
    "src": "src"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config ./src/webpack.config.js --mode production",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config ./src/webpack.config.js --mode development --watch",
    "test": "NODE_OPTIONS=--openssl-legacy-provider npx standard && npx jest"
  },
  "author": "githubtraining",
  "license": "MIT",
  "standard": {
    "env": [
      "jest",
      "browser"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.9.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^5.5.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "standard": "^14.3.1",
    "webpack": "^5.86.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "node-forge": "^1.3.1"
  }
}
